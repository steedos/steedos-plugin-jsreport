{
  "name": "@steedos/plugin-jsreport",
  "version": "1.10.0",
  "description": "Steedos Jsreport报表插件",
  "main": "package.service.js",
  "license": "Apache",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "jsreport-chrome-pdf": "1.5.0",
    "jsreport-core": "2.5.0",
    "jsreport-handlebars": "2.0.2",
    "jsreport-html-to-xlsx": "2.3.1",
    "jsreport-scripts": "2.1.0",
    "jsreport-text": "2.0.0",
    "puppeteer": "1.17.0"
  },
  "scripts": {
    "debug": "npm run compile && nodemon start.js",
    "start": "npm run compile && node start.js",
    "compile": "babel -d main/default src/ && mv main/default/router.js main/default/jsreport.router.js && cp -r src/static lib",
    "prepare": "npm run compile",
    "clear": "rd /s /q node_modules\\@steedos\\auth\\node_modules\\@steedos node_modules\\@steedos\\core\\node_modules\\@steedos node_modules\\@steedos\\objectql\\node_modules\\@steedos node_modules\\@steedos\\plugin-jsreport\\node_modules\\@steedos"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@steedos/objectql": "2.1.38",
    "@steedos/service-package-loader": "^2.1.39",
    "cookies": "0.6.2",
    "cross-fetch": "^3.0.4",
    "css-modules-require-hook": "^4.2.3",
    "moment": "^2.24.0",
    "nodemon": "^1.19.1",
    "steedos-server": "2.1.38"
  }
}
